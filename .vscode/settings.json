{
	"[xml]": {
		"editor.defaultFormatter": "redhat.vscode-xml"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[typescript]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[javascript]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"cSpell.words": [
		"asynciterable",
		"deno",
		"noredirect",
		"Uitspraak",
		"vitalets"
	],
	"deno.enable": true,
	"deno.lint": true,
	"deno.unstable": true,
	"editor.defaultFormatter": "denoland.vscode-deno",
	"typescript.tsserver.experimental.enableProjectDiagnostics": false
}
